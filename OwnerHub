local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/TheMagicPiston2/UI-Libraries/main/ShayrenClient"))() --you can go into the github link and copy all of it and modify it for yourself.
local Window = Library:CreateWindow("Elysian - A Piece [Owner Edition]", Vector2.new(492, 598), Enum.KeyCode.Delete) --you can change your UI keybind
local Legitbot = Window:CreateTab("Legit") --you can rename this tab to whatever you want --you can also change the tabs code, for example "AimingTab" can be changed to "FunnyCoolTab" etc.


local WorkInProgress = Legitbot:CreateSector("Silent-Aim (WIP)", "left")

local ToggleBind = WorkInProgress:AddToggle("Toggle", false, function(e)

end)

ToggleBind:AddKeybind()

local Raging = Window:CreateTab("Rage")

local Haki = Raging:CreateSector("HaoHaki", "left")

Haki:AddButton("Haki Spam - HaoHaki", function(IhateGayPeople)
    loadstring(game:HttpGet("https://raw.githubusercontent.com/TheMagicPiston2/A-Piece/main/HaoHakiSpam"))()
end)

Haki:AddButton("Haki Buy - HaoHaki", function(IhateGayPeople)
    loadstring(game:HttpGet("https://raw.githubusercontent.com/TheMagicPiston2/A-Piece/main/BuyHaoHaki"))()
end)

local Miscs = Raging:CreateSector("Misc", "left")

local CustomStand = Window:CreateTab("Custom Stands!")

local CS = CustomStand:CreateSector("Saitama - String Only", "left")

CS:AddButton("Serious Table Flip", function(IhateGayPeople)
    loadstring(game:HttpGet("https://raw.githubusercontent.com/TheMagicPiston2/A-Piece/main/SeriousTableFlip"))()
end)

CS:AddButton("No Message Serious Flip", function(IhateGayPeople)
    loadstring(game:HttpGet("https://raw.githubusercontent.com/TheMagicPiston2/A-Piece/main/NoSeriousTableFlipMSG"))()
end)

