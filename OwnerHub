    loadstring(game:HttpGet("https://raw.githubusercontent.com/TheMagicPiston2/A-Piece/main/DeathPenalty"))()
end)

CS:AddButton("Serious Sideway Jumps", function(IhateGayPeople)
    loadstring(game:HttpGet("https://raw.githubusercontent.com/TheMagicPiston2/A-Piece/main/SidewayJumps"))()
end)

CS:AddButton("Saitamas Aura", function(IhateGayPeople)
    loadstring(game:HttpGet("https://raw.githubusercontent.com/TheMagicPiston2/A-Piece/main/MenacingAuraV2"))()
end)

CS:AddButton("Gods Aura (Dangerous)", function(IhateGayPeople)
    loadstring(game:HttpGet("https://raw.githubusercontent.com/TheMagicPiston2/A-Piece/main/GodsAura"))()
end)

CS:AddButton("Normal Punch", function(IhateGayPeople)
    loadstring(game:HttpGet("https://raw.githubusercontent.com/TheMagicPiston2/A-Piece/main/NormalPunch"))()
end)

CS:AddButton("Moon Jump", function(IhateGayPeople)
    loadstring(game:HttpGet("https://raw.githubusercontent.com/TheMagicPiston2/A-Piece/main/SeriousMoonJump"))()
end)

